[ -d "$HOME/.nvm" ] && export NVM_DIR="$HOME/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && \. "$NVM_DIR/nvm.sh"  # This loads nvm
# [ -s "$NVM_DIR/bash_completion" ] && \. "$NVM_DIR/bash_completion"  # This loads nvm bash_completion

[ -d "$HOME/.nodenv" ] && export NODENV_DIR="$HOME/.nodenv"
[ -d "$NODENV_DIR/bin" ] && export PATH="$NODENV_DIR/bin:$PATH"
[ -s "$NODENV_DIR/bin/nodenv" ] && eval "$(nodenv init -)"

[ -d "/opt/homebrew/bin/mise" ] && eval "$(mise activate zsh)"
